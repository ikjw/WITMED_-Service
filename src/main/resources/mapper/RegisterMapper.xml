<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.test.dao.IRegisterDao">
    <resultMap id="BaseResultMap" type="com.example.test.entity.Login">
    </resultMap>
    <insert id="register" parameterType="com.example.test.entity.Login">
        insert into login(username,password)
        values (#{username},#{password})
    </insert>
    <insert id="registerDoctor" parameterType="com.example.test.entity.Login">
        insert into login(username,password,role)
        values (#{username},#{password},#{role})
    </insert>
</mapper>
